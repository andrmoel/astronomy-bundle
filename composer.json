{
    "name": "andrmoel/astronomy-bundle",
    "description": "Bundle for astronomical calculations such as position of moon, sun or solar eclipses",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Moeller",
            "email": "andrmoel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Andrmoel\\AstronomyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andrmoel\\AstronomyBundle\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1",
        "phpunit/phpunit": "^7"
    }
}
